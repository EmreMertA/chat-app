ors.map((error, index) => (\n            <Message key={index} error>\n              {error.message}\n            </Message>\n          ))}\n\n        <Message>\n          Have an account? <Link to=\"/login\"> Log in</Link>\n        </Message>\n      </Grid.Column>\n    </Grid>\n  );\n};\n\nexport default SignUp;\n","D:\\projeler\\chat-app\\src\\components\\auth\\Login.jsx",[],"D:\\projeler\\chat-app\\src\\firebase.js",[],"D:\\projeler\\chat-app\\src\\store\\store.js",["46"],"import { createStore, applyMiddleware } from \"redux\";\r\nimport { composeWithDevTools } from \"redux-devtools-extension\";\r\nimport rootReducer from \"./rootReducer\";\r\n\r\nconst store = createStore(rootReducer, {}, composeWithDevTools());\r\n\r\nexport default store;\r\n","D:\\projeler\\chat-app\\src\\store\\rootReducer.js",[],"D:\\projeler\\chat-app\\src\\components\\auth\\PrivateRoute.jsx",[],"D:\\projeler\\chat-app\\src\\components\\Fallback.jsx",["47"],{"ruleId":"48","severity":1,"message":"49","line":6,"column":3,"nodeType":"50","messageId":"51","endLine":6,"endColumn":18},{"ruleId":"52","severity":1,"message":"53","line":46,"column":6,"nodeType":"54","endLine":46,"endColumn":8,"suggestions":"55"},{"ruleId":"56","replacedBy":"57"},{"ruleId":"58","replacedBy":"59"},{"ruleId":"48","severity":1,"message":"60","line":44,"column":9,"nodeType":"50","messageId":"51","endLine":44,"endColumn":22},{"ruleId":"48","severity":1,"message":"61","line":1,"column":23,"nodeType":"50","messageId":"51","endLine":1,"endColumn":38},{"ruleId":"48","severity":1,"message":"62","line":2,"column":42,"nodeType":"50","messageId":"51","endLine":2,"endColumn":46},"no-unused-vars","'firebaseReducer' is defined but never used.","Identifier","unusedVar","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'history'. Either include it or remove the dependency array.","ArrayExpression",["63"],"no-native-reassign",["64"],"no-negated-in-lhs",["65"],"'displayErrors' is assigned a value but never us